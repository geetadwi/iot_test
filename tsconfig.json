{
    "compilerOptions": {
        "target": "es6",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "noFallthroughCasesInSwitch": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        // "noEmit": true,
        "jsx": "react-jsx",
        "baseUrl": "assets",
        "paths": {
            "@App/*": [
                "*"
            ],
            "@Admin/*": [
                "admin/*"
            ]
        }
    },
    "include": [
        "assets/**/*",
        "./declaration.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}

